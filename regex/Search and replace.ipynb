   "source": [
    "import re\n",
    "\n",
    "phone = \"731-752-2601   #phone number\"\n",
    "\n",
    "print(phone, \"\\n\")\n",
    "\n",
    "# remove python comments\n",
    "num = re.sub(r'#.*$', \"\", phone)\n",
    "print(\"Phone Number : \", num)\n",
    "\n",
    "# remove unwanted symbols\n",
    "num = re.sub(r'\\D', \"\", phone)    \n",
    "print(\"Phone Number : \", num)"
